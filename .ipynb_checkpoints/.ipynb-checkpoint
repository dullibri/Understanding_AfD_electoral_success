   "source": [
    "import pandas as pd\n",
    "import numpy as np\n",
    "import re \n",
    "\n",
    "\n",
    "df = pd.read_csv('fb441e62df2d58994928907a91895ec62c2c42e6cd075c2700843b89.csv',\n",
    "                date_parser=True)\n",
    "\n",
    "# Filtering out the leap-year days\n",
    "df = df.sort_values(by=['Date'])\n",
    "\n",
    "df = df[-df['Date'].str.contains('[0-9]-02-29')]\n",
    "\n",
    "\n",
    "# in order to get month-day records transform every date to 2015(as this is the plot it needs to be \n",
    "# inserted to)\n",
    "# in order to get month-day records transform every date to 2015(as this is the plot it needs to be \n",
    "# inserted to)\n",
    "df['DateStr'] = df.Date.apply(str)\n",
    "df.DateStr = df.DateStr.apply(lambda i: re.sub(' 00:00:00','', i))\n",
    "df.DateStr = df.DateStr.apply(lambda i: re.sub('20[0-9]{2}','2015', i))\n",
    "\n",
    "df['Date'] = pd.to_datetime(df['Date'])\n",
    "\n",
    "df.set_index('Date', inplace=True)\n",
    "df05_14 = df.loc['2005-01-01':'2014-01-01']\n",
    "df15 = df['2015-01-01':'2015-12-31']\n",
    "\n",
    "df05_14.DateStr = pd.to_datetime(df05_14.DateStr)\n",
    "df05_14.set_index('DateStr', inplace=True)\n",
    "\n",
    "res = df05_14['Data_Value'].resample('d').agg([min, max])#.resample('Y')\n",
    "res.rename(columns={'min':'Low','max':'High'},inplace=True)\n",
    "res = res[['High', 'Low']]\n",
    "Maxima = res['High']\n",
    "Minima = res['Low']\n",
    "\n",
    "\n",
    "\n",
    "df15N = df15.merge(res, right_index=True, left_index=True)\n",
    "#df15N['BreakHigh'] = df15N\n",
    "df15N['High_ind']=(-df15N['Data_Value']+df15N['High'])<0\n",
    "df15N['Low_ind']=(-df15N['Data_Value']+df15N['Low'])>0\n",
    "Highs = df15N[df15N.High_ind==True]\n",
    "Lows = df15N[df15N.Low_ind==True]\n",
    "\n",
    "\n",
    "\n",
    "\n"
   ]
